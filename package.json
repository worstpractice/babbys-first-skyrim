{
  "dependencies": {
    "normalize.css": "8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "three": "0.131.3",
    "zustand": "3.5.7"
  },
  "devDependencies": {
    "@babel/runtime": "7.14.8",
    "@snowpack/plugin-react-refresh": "2.5.0",
    "@snowpack/plugin-typescript": "1.2.1",
    "@types/dat.gui": "0.7.7",
    "@types/react": "17.0.16",
    "@types/react-dom": "17.0.9",
    "@types/snowpack-env": "2.3.4",
    "@types/three": "0.130.1",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "eslint": "7.32.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-unicorn": "35.0.0",
    "prettier": "2.3.2",
    "snowpack": "3.8.3",
    "typescript": "4.3.5"
  },
  "scripts": {
    "build": "snowpack build",
    "fmt": "prettier -u --config .prettierrc -w src/**/*.{d.ts,ts,tsx,json}",
    "lint": "eslint ./src --ext .d.ts,.ts,.tsx",
    "lint:fix": "eslint ./src --ext .d.ts,.ts,.tsx --fix",
    "nuke": "\"rd /q /s .\\node_modules\\.cache\\snowpack\" | cmd",
    "start": "pnpm nuke && snowpack dev",
    "tidy": "pnpm run lint:fix && pnpm run fmt"
  },
  "type": "module"
}
